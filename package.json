{
  "name": "giffgaff-status",
  "private": true,
  "description": "Having patchy signal or got no data? Check the status of giffgaff easily from one page. This site updates automatically so you don't miss any issues.",
  "version": "0.2.0",
  "author": "David Wheatley <giffgaffstatus@davwheat.dev>",
  "dependencies": {
    "@fontsource/poppins": "^4.2.2",
    "clsx": "^1.1.1",
    "gatsby": "^3.4.0",
    "gatsby-image": "^3.4.0",
    "gatsby-plugin-less": "^5.4.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-preact": "^5.4.0",
    "gatsby-plugin-preload-fonts": "^2.4.0",
    "gatsby-plugin-react-head": "https://github.com/davwheat/gatsby-plugin-react-head",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-sharp": "^3.4.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.22",
    "gatsby-plugin-webpack-size": "^2.0.1",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-head": "^3.4.0",
    "react-helmet": "^6.1.0",
    "react-portal": "^4.2.1"
  },
  "devDependencies": {
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "echo 'HAVE YOU PRELOADED FONTS?' && cross-env NODE_ENV=production gatsby build",
    "build:ci": "cross-env NODE_ENV=production gatsby build --verbose",
    "preload-fonts": "gatsby-preload-fonts",
    "develop": "gatsby develop -p 8080",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy:pages": "yarn build && gh-pages -b prod -d public -m \"Deploy website\""
  }
}
